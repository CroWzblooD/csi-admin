generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event {
  id          String   @id @default(cuid())
  name        String
  description String
  venue       String
  isPaid      Boolean
  isOnline    Boolean
  guest       String?
  eventDate   DateTime
  eventTime   String
  banner      String
  imageUrls   String[]
  isPrivate   Boolean
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model Admin {
  id       String @id @default(cuid())
  name     String
  branch   String
  batch    String
  position String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
